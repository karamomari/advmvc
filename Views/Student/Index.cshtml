@model List<Student>



<table class="table table-dark table-hover">
    <thead>
        <tr>
       
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Image</th>
        </tr>
    </thead>
    <tbody>
   @foreach (var item in Model)
    {
        <tr>
           
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>
           <img src="/Image/@item.Image" alt="Alternate Text" width="60px" />
            </td>
        </tr>
    }
    </tbody>
</table>